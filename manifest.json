{

  "manifest_version": 2,
  "name": "WikiRecorder",
  "version": "1.0",

  "description": "Record clicked links during Wikigames sessions and display them on HTML page.",

  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"

  },

  "permissions": [
          "history"
        ],

  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"],
      "js": ["theWikiChallengeRecorder.js"]
    }
  ]

}
